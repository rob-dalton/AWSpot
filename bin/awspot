#!/usr/bin/env python
import sys

if __name__ == "__main__":
    try:
        resource_type = sys.argv[1]
    except:
        print("ERROR: No resource type specified")

    # determine script to call (ec2.py)
    if resource_type == "ec2":
        import awspot.exec.ec2
    elif resource_type == "emr":
        import awspot.exec.emr
    else:
        print("ERROR: No resource type specified") 

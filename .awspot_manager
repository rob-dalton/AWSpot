# AWSPOT MANAGER ENV FILE
# env vars and aliases for awspot manager

# vars
export AWSPOT_MANAGER_FILE='.awspot_manager'
export AWSPOT_DIR='/Users/robertdalton/web-projects/awspot-manager/'
export AWSPOT_USER='ec2-default'
